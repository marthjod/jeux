<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
 <display-name>JeuxWeb</display-name>
 <servlet>
  <description>JAX-RS Tools Generated - Do not modify</description>
  <servlet-name>JAX-RS Servlet</servlet-name>
  <servlet-class>javax.ws.rs.core.Application</servlet-class>
  <load-on-startup>1</load-on-startup>
 </servlet>
 <servlet>
  <servlet-name>CreateGroupServlet</servlet-name>
  <servlet-class>de.fhb.jeux.controller.CreateGroupServlet</servlet-class>
 </servlet>
 <servlet>
  <display-name>CreatePlayersServlet</display-name>
  <servlet-name>CreatePlayersServlet</servlet-name>
  <servlet-class>de.fhb.jeux.controller.CreatePlayersServlet</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>JAX-RS Servlet</servlet-name>
  <url-pattern>/rest/*</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>CreateGroupServlet</servlet-name>
  <url-pattern>/create-group</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>CreatePlayersServlet</servlet-name>
  <url-pattern>/create-players</url-pattern>
 </servlet-mapping>
 <welcome-file-list>
  <welcome-file>/index.html</welcome-file>
 </welcome-file-list>
</web-app>
